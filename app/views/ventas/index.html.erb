<div class="container">
  <div class="cards">
      <% @ventas.each do |venta|  %>
        <div class="card-trip img-fluid">
         <%= cl_image_tag(venta.photo.key,:aspect_ratio=>"1.0", width: 350, height: 300,:crop=>"fill") %>
         <div class="card-trip-infos">
            <a><%= link_to " #{venta.modelo}"  , venta_path(venta) %></a>
            <p> Marca: <%= venta.marca %></h2>
            <p> disponibilidad: <%= venta.disponible %></p>
            <p> precio: <%= venta.precio %></p>
            <p></p>
            <a href="<%= venta.link.to_s%>" style="font-size: 16px">Ficha tecnica </a>
          </div>
        </div>
    <% end %>
  </div>
